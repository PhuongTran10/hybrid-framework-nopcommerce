<?xml version="1.0" encoding="UTF-8"?>
<suite name="Nopcommerce UI Automation Testing" parallel="tests" thread-count="2">
	<parameter name="envName" value="grid" />
	<parameter name="serverName" value="dev" />
	<test name="Run on Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="osName" value="windows" />
		<parameter name="ipAddress" value="localhost" />
		<parameter name="port" value="4444" />
		<classes>
			<class name="com.nopcommerce.grid.Level_24_Grid" />
		</classes>
	</test>
	<test name="Run on Firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="osName" value="windows" />
		<parameter name="ipAddress" value="localhost" />
		<parameter name="port" value="4444" />
		<classes>
			<class name="com.nopcommerce.grid.Level_24_Grid" />
		</classes>
	</test>
</suite>